{
  "name": "fashionaire",
  "version": "1.0.0",
  "description": "AI-powered fashion styling application",
  "main": "index.html",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production webpack --mode=production",
